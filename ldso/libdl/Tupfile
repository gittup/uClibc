include_rules

CFLAGS += -fPIC
CFLAGS += -DSHARED
CFLAGS += -DNOT_IN_libc
CFLAGS += -DIS_IN_rtld
CFLAGS += -I../include
CFLAGS += -I../ldso/i386
CFLAGS += $(UCLIBC_LINUX_INCLUDES)
CFLAGS += -DUCLIBC_RUNTIME_PREFIX='"/usr/i386-linux-uclibc/"'
CFLAGS += -DUCLIBC_LDSO=\"ld-uClibc.so.0\"
CFLAGS += -DLDSO_ELFINTERP=\"i386/elfinterp.c\"

: foreach libdl.c |> !cc_proper |> %B.o {objs}
: {objs} |> !ar |> libdl_so.a
