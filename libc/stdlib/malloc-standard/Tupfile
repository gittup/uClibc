include_rules

obj-@(UCLIBC_MALLOC_STANDARD) += malloc.c
obj-@(UCLIBC_MALLOC_STANDARD) += calloc.c
obj-@(UCLIBC_MALLOC_STANDARD) += realloc.c
obj-@(UCLIBC_MALLOC_STANDARD) += free.c
obj-@(UCLIBC_MALLOC_STANDARD) += memalign.c
obj-@(UCLIBC_MALLOC_STANDARD) += mallopt.c
obj-@(UCLIBC_MALLOC_STANDARD) += mallinfo.c

CFLAGS += $(UCLIBC_LINUX_INCLUDES)

: foreach $(obj-y) | $(compiler_proper) |> ^ CC %f^ $(CC) -c %f -o %o $(CFLAGS) $(CFLAGS_%e) |> %B.o
